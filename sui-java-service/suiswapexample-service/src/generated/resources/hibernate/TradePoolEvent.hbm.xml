<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.test.suiswapexample.domain.tradepool">
  <class name="AbstractTradePoolEvent" table="trade_pool_event" abstract="true">
    <composite-id name="tradePoolEventId" class="org.test.suiswapexample.domain.tradepool.TradePoolEventId">
      <key-property name="id">
        <column name="id" sql-type="VARCHAR(66)" />
      </key-property>
      <key-property name="version">
        <column name="version" sql-type="DECIMAL(20,0)" />
      </key-property>
    </composite-id>
    <discriminator column="event_type" type="string" />
    <property name="createdBy" column="created_by" />
    <property name="createdAt" column="created_at" />
    <property name="commandId" column="command_id" />
    <property name="suiTimestamp">
      <column name="sui_timestamp" />
    </property>
    <property name="suiTxDigest">
      <column name="sui_tx_digest" length="50" />
    </property>
    <property name="suiEventSeq">
      <column name="sui_event_seq" sql-type="DECIMAL(20,0)" />
    </property>
    <property name="suiPackageId">
      <column name="sui_package_id" sql-type="VARCHAR(66)" />
    </property>
    <property name="suiTransactionModule">
      <column name="sui_transaction_module" length="100" />
    </property>
    <property name="suiSender">
      <column name="sui_sender" sql-type="VARCHAR(66)" />
    </property>
    <property name="suiType">
      <column name="sui_type" length="200" />
    </property>
    <property name="status">
      <column name="status" sql-type="CHAR(1)" />
    </property>
    <property name="commandType" column="command_type" length="50" />
    <subclass name="AbstractTradePoolEvent$TradePoolClobEvent" discriminator-value="TradePoolClobEvent">
      <property name="dynamicPropertiesLob">
        <column name="dynamic_properties_lob" sql-type="VARCHAR(2000)" />
      </property>
      <subclass name="AbstractTradePoolEvent$TradePoolInitialized" discriminator-value="TradePoolInitialized" />
      <subclass name="AbstractTradePoolEvent$TradePoolWithEmptyXReserveInitialized" discriminator-value="TradePoolWithEmptyXReserveInitialized" />
      <subclass name="AbstractTradePoolEvent$SellPoolInitialized" discriminator-value="SellPoolInitialized" />
      <subclass name="AbstractTradePoolEvent$BuyPoolInitialized" discriminator-value="BuyPoolInitialized" />
      <subclass name="AbstractTradePoolEvent$PoolExchangeRateUpdated" discriminator-value="PoolExchangeRateUpdated" />
      <subclass name="AbstractTradePoolEvent$PoolXTokenAdded" discriminator-value="PoolXTokenAdded" />
      <subclass name="AbstractTradePoolEvent$PoolXTokenRemoved" discriminator-value="PoolXTokenRemoved" />
      <subclass name="AbstractTradePoolEvent$PoolYReserveDeposited" discriminator-value="PoolYReserveDeposited" />
      <subclass name="AbstractTradePoolEvent$PoolYReserveWithdrawn" discriminator-value="PoolYReserveWithdrawn" />
      <subclass name="AbstractTradePoolEvent$PoolDestroyed" discriminator-value="PoolDestroyed" />
      <subclass name="AbstractTradePoolEvent$PoolYSwappedForX" discriminator-value="PoolYSwappedForX" />
      <subclass name="AbstractTradePoolEvent$PoolXSwappedForY" discriminator-value="PoolXSwappedForY" />
    </subclass>
  </class>
</hibernate-mapping>
