<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.test.suiswapexample.domain.tradepool">
  <class name="AbstractTradePoolX_ReserveItemEvent" table="trade_pool_x_reserve_item_event" abstract="true">
    <composite-id name="tradePoolX_ReserveItemEventId" class="org.test.suiswapexample.domain.tradepool.TradePoolX_ReserveItemEventId">
      <key-property name="tradePoolId">
        <column name="trade_pool_id" sql-type="VARCHAR(66)" />
      </key-property>
      <key-property name="key">
        <column name="key_" sql-type="VARCHAR(66)" />
      </key-property>
      <key-property name="version">
        <column name="version" sql-type="DECIMAL(20,0)" />
      </key-property>
    </composite-id>
    <discriminator column="event_type" type="string" />
    <property name="createdBy" column="created_by" />
    <property name="createdAt" column="created_at" />
    <property name="commandId" column="command_id" />
  </class>
</hibernate-mapping>
