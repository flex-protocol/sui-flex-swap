<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.test.suiswapexample.domain.tokenpair">
  <class name="AbstractTokenPairX_ReserveItemEvent" table="token_pair_x_reserve_item_event" abstract="true">
    <composite-id name="tokenPairX_ReserveItemEventId" class="org.test.suiswapexample.domain.tokenpair.TokenPairX_ReserveItemEventId">
      <key-property name="tokenPairId">
        <column name="token_pair_x_reserve_item_id_token_pair_id" sql-type="VARCHAR(66)" />
      </key-property>
      <key-property name="key">
        <column name="token_pair_x_reserve_item_id_key" sql-type="VARCHAR(66)" />
      </key-property>
      <key-property name="version">
        <column name="version" sql-type="DECIMAL(20,0)" />
      </key-property>
    </composite-id>
    <discriminator column="event_type" type="string" />
    <property name="createdBy" column="created_by" />
    <property name="createdAt" column="created_at" />
    <property name="commandId" column="command_id" />
  </class>
</hibernate-mapping>
